<?xml version="1.0" ?>
<odoo>

    <record id="estate_doctor_action" model="ir.actions.act_window">
        <field name="name">Doctors</field>
        <field name="res_model">estate.doctor</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Click create button to add your first doctor
            </p>
        </field>
    </record>

    <record id="estate_doctor_tree_view" model="ir.ui.view">
        <field name="name">Doctors</field>
        <field name="model">estate.doctor</field>
        <field name="arch" type="xml">
            <form>
                <field name="my_gender" />
            </form>
        </field>
    </record>

</odoo>